cmd_busybox__ = ccache $(CC) -o $@.js -Wl,--start-group -s ERROR_ON_UNDEFINED_SYMBOLS=0 -Os $(SRCDIR)/em-shell.c -include $(SRCDIR)/em-shell.h --js-library $(SRCDIR)/em-shell.js $(CFLAGS) $(CFLAGS_busybox) $(LDFLAGS) $(EM_LDFLAGS) $(EXTRA_LDFLAGS) $(core-y) $(libs-y) $(patsubst %,-l%,$(subst :, ,$(LDLIBS))) -Wl,--end-group && cp $@.js $@
